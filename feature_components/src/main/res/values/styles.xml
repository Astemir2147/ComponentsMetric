<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="ContractsTextStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ContractsTextStyle.Secondary" parent="ContractsTextStyle">
        <item name="android:textColor">@color/gray</item>
    </style>

    <style name="Widget.App.Button.OutlinedButton.IconOnly" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:textSize">15sp</item>
        <item name="backgroundTint">@android:color/transparent</item>
        <item name="strokeColor">@color/gray</item>
        <item name="strokeWidth">1dp</item>
        <item name="rippleColor">#DFE4D6</item>

        <item name="android:textColor">@color/colorOnSurface</item>
    </style>
</resources>